{
  "name": "frontend-erp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env ENV_TYPE=local node operations.js && next",
    "build": "cross-env ENV_TYPE=local node operations.js && next build",
    "start": "next start",
    "test": "jest",
    "lint": "next lint",
    "format": "prettier --write ./src",
    "type-check": "tsc",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check ./src",
    "check-lint": "eslint ./src --ext ts --ext tsx --ext js",
    "test-all": "yarn check-format && yarn check-lint && yarn check-types && yarn build",
    "prepare": "husky install",
    "pre-commit": "lint-staged --allow-empty --concurrent false"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.6",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "highcharts": "^10.3.1",
    "highcharts-react-official": "^3.1.0",
    "lodash": "^4.17.21",
    "next": "latest",
    "react": "^18.2.0",
    "react-calendar": "^4.0.0",
    "react-day-picker": "^8.3.7",
    "react-dom": "^18.1.0",
    "react-redux": "^7.2.0",
    "react-select": "^5.6.1",
    "sass": "^1.54.4",
    "sharp": "^0.31.1",
    "simple-react-calendar": "^2.2.8"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.2",
    "@heroicons/react": "^2.0.11",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.5",
    "@types/react-redux": "^7.1.18",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.0.1",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "globby": "^13.1.2",
    "husky": "^8.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^27.2.0",
    "jest-css-modules-transform": "^4.2.0",
    "postcss": "^8.4.16",
    "postcss-import": "^15.0.0",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.4"
  },
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.0.0 <17.0.0"
  }
